    secondaryAction={
                                                   <Box >
                                                       {
                                                           product.cpt > 0 &&
                                                           <IconButton onClick={() => {removeFromCart(index1,index2)}}>
                                                               <RemoveIcon />
                                                           </IconButton>
                                                       }
                                                       <IconButton onClick={() => {addToCart(index1,index2)}}>
                                                           <AddIcon />
                                                       </IconButton>
                                                   </Box>
                                               }

                                               <ListItemButton
                                                                                              onClick={() => {handleClickOpen(index1,index2)}}
                                                                                          sx={{p:0, ml:0.5, mr:0.5, mt:0, mb:0}}>
                                                                                              {
                                                                                                  product.cpt !== 0 &&
                                                                                                  <ListItemIcon>
                                                                                                      <IconButton sx={{alignContent: 'center'}}>
                                                                                                          <RemoveIcon />
                                                                                                      </IconButton>
                                                                                                  </ListItemIcon>
                                                                                              }

                                                                                              <ListItemAvatar >
                                                                                                  <Avatar
                                                                                                      alt={`image :${product.productName}`}
                                                                                                      src={product.image}
                                                                                                  />
                                                                                              </ListItemAvatar>
                                                                                              <ListItemText primary={product.productName}  secondary={<Ingredient product={product}/>} />
                                                                                          </ListItemButton>

                                                                                          <Stack direction="row" spacing={0} key={index3}>
                                                                                                                                                                 {
                                                                                                                                                                     ingredient.type === 'none' && ingredient.validate === true &&
                                                                                                                                                                     <Chip component="span" label={ingredient.ingredientName}/>
                                                                                                                                                                 }
                                                                                                                                                                 {
                                                                                                                                                                     ingredient.type === 'choice' && ingredient.validate === false &&
                                                                                                                                                                     <Chip component="span" sx={{m: 0.1}}  label={ingredient.ingredientName}/>
                                                                                                                                                                 }
                                                                                                                                                                 {
                                                                                                                                                                     ingredient.type === 'choice' && ingredient.validate === true &&
                                                                                                                                                                     ingredient.choice && ingredient.choice.map((choice, index4) => (
                                                                                                                                                                         <div>
                                                                                                                                                                             {
                                                                                                                                                                                 choice.status && <Chip component="span" sx={{m: 0.1}}  label={choice.choiceName}/>
                                                                                                                                                                             }
                                                                                                                                                                         </div>
                                                                                                                                                                     ))
                                                                                                                                                                 }
                                                                                                                                                             </Stack>